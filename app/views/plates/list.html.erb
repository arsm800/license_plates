<h1>Plates<h1>

<table class="table table-hover">
  <% @plates.each do |plate| %>
    <tr>
      <td> <%= plate.number %> </td>
      <td> <%= plate.year %> </td>
      <td> <%= plate.style %> </td>
      <td> <%= plate.source %> </td>
      <td> <%= plate.date_acquired %> </td>
    </tr>
  <% end %>

</table>
